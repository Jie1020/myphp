<?php

function calculateFunc($num1, $operations, $num2){
    $ans;
    switch($operations){
        case "add":
            $ans = $num1 + $num2;
            break;
        case "subtract":
            $ans = $num1 - $num2;
            break;
        default:
        $result = "Output Error";
        break;
    }
    return $ans;
}